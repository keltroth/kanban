  },
  "files": [
    "kanban.js"
  ],
  "main": "kanban.js",
  "devDependencies": {
    "@keltroth/emitter": "^1.0.7",
    "esbuild": "^0.17.11",
    "npx": "^10.2.2"
  },
  "author": "Django Janny",
  "license": "MIT"
}
